{
  "sources": [
    {
      "web": "http://www4.unfccc.int/submissions/indc/Submission%20Pages/submissions.aspx",
      "name": "UNFCCC website with INDCs as submitted by Parties"
    }
  ],
  "resources": [
    {
      "name": "indcs",
      "path": "data/indcs.csv",
      "format": "csv",
      "mediatype": "text/csv",
      "schema": {
        "fields": [
          {
            "name": "Code",
            "type": "string",
            "description": "ISO3 code of Party, EUU for European Union"
          },
          {
            "name": "Party",
            "type": "string",
            "description": "Name of Party"
          },
          {
            "name": "FileType",
            "type": "string",
            "description": "Kind of document"
          },
          {
            "name": "Language",
            "type": "string",
            "description": "Document language"
          },
          {
            "name": "Filename",
            "type": "string",
            "description": "Unified filename"
          },
          {
            "name": "SubmissionDate",
            "type": "datetime",
            "description": "Date of submission"
          },
          {
            "name": "EncodedAbsUrl",
            "type": "string",
            "description": "URL of original document"
          },
          {
            "name": "OriginalFilename",
            "type": "string",
            "description": "Full name of original document"
          }
        ]
      }
    }
  ],
  "name": "indcs",
  "title": "Intended Nationally Determined Contributions (INDCs)",
  "description": "Data Package of Intended Nationally Determined Contributions (INDCs)",
  "repository": "git://github.com/openclimatedata/indcs.git"
}
